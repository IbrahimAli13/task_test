<p style="color: green"><%= notice %></p>

<h1>Coordinators</h1>

<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Email</th>
      <th scope="col">Name</th>
      <th scope="col">Phone</th>
      <th scope="col">See</th>
    </tr>
  </thead>
  <tbody>
  <% @coordinators.each do |coordinator| %>
    <tr>
      <th scope="row"><%= coordinator.id %></th>
      <td><%= coordinator.email %></td>
      <td><%= coordinator.name %></td>
      <td><%= coordinator.phone %></td>
      <td><%= link_to "Edit", coordinator_path(coordinator.id), class: "btn btn-light"  %></td>
    </tr>
    <tr>
<%end%>
  </tbody>
</table>

<%= link_to "New coordinator", new_coordinator_path, class: "btn btn-light" %>
